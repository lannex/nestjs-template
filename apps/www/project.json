{
  "name": "www",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/www/src",
  "projectType": "application",
  "targets": {
    "webpack": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "target": "node",
        "compiler": "swc",
        "outputPath": "dist/apps/www",
        "main": "apps/www/src/main.ts",
        "tsConfig": "apps/www/tsconfig.app.json",
        "assets": [
          "apps/www/src/assets"
        ]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/www/webpack/environment.ts",
              "with": "apps/www/webpack/environment.prod.ts"
            }
          ]
        }
      }
    },
    "watch": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "www:webpack"
      },
      "configurations": {
        "production": {
          "buildTarget": "www:webpack:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/www/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/www/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "configurations": {
        "local": {
          "command": "APP_NAME=www NODE_ENV=local nx run www:watch"
        },
        "dev": {
          "command": "APP_NAME=www nx run www:watch"
        },
        "prod": {
          "command": "APP_NAME=www nx run www:watch --configuration production"
        }
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "configurations": {
        "dev": {
          "command": "APP_NAME=www nx run www:webpack"
        },
        "prod": {
          "command": "APP_NAME=www nx run www:webpack --configuration production"
        }
      }
    }
  },
  "tags": []
}
